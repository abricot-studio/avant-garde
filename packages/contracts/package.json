{
  "name": "@avant-garde/contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile && npm run scripts:publish",
    "compile": "hardhat compile",
    "node": "hardhat node",
    "test": "npm run test:coverage",
    "test:unit": "hardhat test --logs",
    "test:coverage": "hardhat coverage",
    "lint": "eslint \"**/*.{js,ts}\" && solhint src/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix src/**/*.sol",
    "scripts": "hardhat --network localhost run",
    "deploy:localhost": "hardhat --network localhost deploy",
    "deploy:rinkeby": "hardhat --network rinkeby deploy",
    "deploy:kovan": "hardhat --network kovan deploy",
    "verify:rinkeby": "hardhat verify --network rinkeby 0xADF506189b0aC008fBc58041512D1BC3F6652d41 '0xE4D29ec42F4057EfF92c9124c82844b2689f9C6d' '0x91a5D3bCe5317D5579E780d67021fEc692575d1d'",
    "verify:kovan": "hardhat verify --network kovan 0xD5bC431D0CEF8747E4898Aa76f8956646084302a '0xE4D29ec42F4057EfF92c9124c82844b2689f9C6d' '0x91a5D3bCe5317D5579E780d67021fEc692575d1d'",
    "scripts:seed": "npm run scripts ./scripts/seed.ts"
  },
  "author": "Avant Garde",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
